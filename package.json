{
  "name": "backend-kit",
  "author": "FruitieX",
  "description": "Get started with backend development",
  "license": "MIT",
  "bugs": "https://github.com/FruitieX/backend-kit/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FruitieX/backend-kit.git"
  },
  "keywords": [
    "Node",
    "Postgres",
    "Knex",
    "Jest"
  ],
  "scripts": {
    "start": "npm run build && node dist/backend_kit.js",
    "build": "mkdir -p dist && babel src --out-dir dist --source-maps",
    "watch": "cross-env NODE_ENV=development nodemon --exec babel-node src/backend_kit.js",
    "register:admin": "babel-node bin/register_admin.js",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "node_modules/.bin/knex --knexfile db/knexfile.js migrate:latest",
    "db:rollback": "node_modules/.bin/knex --knexfile db/knexfile.js migrate:rollback",
    "db:seed": "node_modules/.bin/knex --knexfile db/knexfile.js seed:run",
    "db:wipe": "babel-node bin/wipe_db.js",
    "db:init": "npm run db:wipe && npm run db:migrate && npm run db:seed",
    "prettier": "prettier --single-quote --trailing-comma all --write \"{,!(dist|node_modules)/**/}*.js\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "version": "0.0.1",
  "main": "src/server.js",
  "private": true,
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "boom": "^5.2.0",
    "cross-env": "^5.0.5",
    "dotenv": "^4.0.0",
    "faker": "^4.1.0",
    "fs-sync": "^1.0.4",
    "glue": "^4.2.0",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.6.0",
    "hapi-auth-jwt2": "^7.3.0",
    "hapi-qs": "^1.1.3",
    "hapi-routes-relative": "^2.2.5",
    "hoek": "^4.2.0",
    "jimp": "^0.2.28",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.3",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "node-uuid": "^1.4.8",
    "pg": "^6.4.2",
    "prompt": "^1.0.0",
    "simple-fixtures": "^1.0.1",
    "white-out": "^2.0.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "chokidar-cli": "^1.2.0",
    "code": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lab": "^14.1.0",
    "lab-babel": "^1.1.1",
    "lint-staged": "^4.1.3",
    "mock-knex": "^0.3.7",
    "nodemon": "^1.9.2",
    "prettier": "^1.6.1"
  }
}
